@section scripts{


    <script type="text/javascript">
        (function ($) {
            'use strict';
            $('#collapseOne, #collapseTwo, #collapseThree, #collapseFour, #collapseFive, #collapseSix, #collapseSeven').on({
                'show.bs.collapse': function () {
                    $('a[href="#' + this.id + '"] span.glyphicons-chevron-down')
                        .removeClass('glyphicons-chevron-down')
                        .addClass('glyphicons-chevron-up');
                },
                'hide.bs.collapse': function () {
                    $('a[href="#' + this.id + '"] span.glyphicon-chevron-up')
                        .removeClass('glyphicons-chevron-up')
                        .addClass('glyphicons-chevron-down');
                }
            });
        })(jQuery);

        function loadEmployees(selecter) {
            var options = $("#listEmployee > option").clone();
            $('#' + selecter + '').append(options);
        }

        var orderId;
        function getOrderId(Id) {
            $('#OrderId').val(Id);
            loadOrderData(Id);
            $('#addNewCompany-Modal').modal('toggle');
        }
        function loadOrderData(Id) {
            var finalTotalAmount = 0;
            var finalTotalBundles = 0;
            if (Id) {
                $.get('/CPC/VaultCustodian/GetVaultCustodian/' + Id, function (response) {
                    if (response) {
                        $('#ProjectBranch').val(response.BranchName);
                        $('#ProjectBranchId').val(response.ProjectBranchId);
                        $('#CityId').val(response.CityId);
                        $('#CityName').val(response.CityName);
                        $('#OrderNumber').val(response.OrderNumber);
                        $('#OrderBookingId').val(response.OrderBookingId);

                        console.log(response.Details);
                        $.each(response.Details, function (index, item) {
                            //debugger;
                            if (item.DenominationTitle == '10') {
                                $("#numberOfPacketsThree").val(item.NumberOfBundles);
                                $("#totalValueThree").val(item.TotalValue);
                                $("#SealNoThree").val(item.SealNo);

                                $("#tenDenomBundle").text(item.NumberOfBundles);
                                $("#tenDenomAmount").text(item.TotalValue);
                                $("#tenDenomSeal").text(item.SealNo);

                                $("#inptenDenomBundle0").val(item.NumberOfBundles);
                                $("#inptenDenomAmount0").val(item.TotalValue);

                                $("#inptenOrderbookingId").val(response.OrderBookingId);
                                loadEmployees('inptenSorter0');
                               
                                finalTotalBundles += parseInt(item.NumberOfBundles);
                                finalTotalAmount += parseInt(item.TotalValue);
                            }
                            else if (item.DenominationTitle == '20') {
                                $("#numberOfPacketsFour").val(item.NumberOfBundles);
                                $("#totalValueFour").val(item.TotalValue);
                                $("#SealNoFour").val(item.SealNo);

                                $("#twentyDenomBundle").text(item.NumberOfBundles);
                                $("#twentyDenomAmount").text(item.TotalValue);
                                $("#twentyDenomSeal").text(item.SealNo);

                                $("#inptwentyDenomBundle0").val(item.NumberOfBundles);
                                $("#inptwentyDenomAmount0").val(item.TotalValue);

                                $("#inptwentyOrderbookingId").val(response.OrderBookingId);
                                loadEmployees('inptwentySorter0');

                                finalTotalBundles += parseInt(item.NumberOfBundles);
                                finalTotalAmount += parseInt(item.TotalValue);
                            }
                            else if (item.DenominationTitle == '50') {
                                $("#numberOfPacketsFive").val(item.NumberOfBundles);
                                $("#totalValueFive").val(item.TotalValue);
                                $("#SealNoFive").val(item.SealNo);

                                var options = $("#listEmployee > option").clone();
                                $("#mytableConsolidated > tbody > tr #sorterEmployeeFive").append(options);

                                finalTotalBundles += parseInt(item.NumberOfBundles);
                                finalTotalAmount += parseInt(item.TotalValue);
                            }
                            else if (item.DenominationTitle == '100') {
                                $("#numberOfPacketsSix").val(item.NumberOfBundles);
                                $("#totalValueSix").val(item.TotalValue);
                                $("#SealNoSix").val(item.SealNo);

                                var options = $("#listEmployee > option").clone();
                                $("#mytableConsolidated > tbody > tr #sorterEmployeeSix").append(options);

                                finalTotalBundles += parseInt(item.NumberOfBundles);
                                finalTotalAmount += parseInt(item.TotalValue);
                            }
